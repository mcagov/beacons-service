{
  "name": "beacons_service_formatting",
  "description": "Beacons Service API formatting scripts and dependencies",
  "scripts": {
    "format": "prettier --write './**/*.{java,md,json,yml,js,jsx,json,ts,tsx,yml,md,html,scss,css}'",
    "format:check": "prettier --check './**/*.{java,md,json,yml,js,jsx,json,ts,tsx,yml,md,html,scss,css}'",
    "prepare": "husky install"
  },
  "devDependencies": {
    "husky": "^7.0.0",
    "prettier": "^2.4.1",
    "prettier-plugin-java": "1.3.1",
    "pretty-quick": "^3.1.3"
  },
  "engines": {
    "node": ">=12.20.x <=14.15.x",
    "npm": "6.14.x"
  }
}
