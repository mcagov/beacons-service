{
  "name": "beacons-backoffice",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "cp .env.example .env",
    "start": "react-scripts start",
    "start:no-api-stub": "REACT_APP_NO_API_STUB=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:watch": "react-scripts test --watch",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "format": "prettier --write '**/*.{js,jsx,json,ts,tsx,yml,md,html,scss,css}'",
    "format:check": "prettier --check '**/*.{js,jsx,json,ts,tsx,yml,md,html,scss,css}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@azure/msal-browser": "^2.16.1",
    "@azure/msal-react": "^1.0.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.172",
    "@types/node": "16.4.13",
    "@types/react": "^17.0.10",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.29.2",
    "axios": "^0.21.1",
    "date-fns": "^2.21.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fontsource-roboto": "^4.0.0",
    "formik": "^2.2.9",
    "husky": "^4.3.8",
    "inflected": "^2.1.0",
    "jest-canvas-mock": "^2.3.1",
    "lint-staged": "^11.1.2",
    "lodash": "^4.17.21",
    "material-table": "^1.69.3",
    "miragejs": "^0.1.41",
    "node-sass": "^5.0.0",
    "prettier": "^2.3.2",
    "prettier-plugin-organize-imports": "^2.3.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "typescript": "^4.3.5",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.0"
  },
  "engines": {
    "node": ">=12.20.x <=14.15.x",
    "npm": "6.14.x"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "*.{js, jsx, ts, tsx}": "eslint --cache --fix",
    "*.{js,jsx,json,ts,tsx,yml,md,html,scss,css}": "prettier --write",
    "*.{tf}": "terraform fmt --recursive"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": ".",
  "jest": {
    "clearMocks": true
  },
  "devDependencies": {
    "@types/uuid": "^8.3.1"
  }
}
